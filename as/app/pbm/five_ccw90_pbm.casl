;;; 「五」を表す画像を反時計方向へ90度回転させた結果をPBMファイルとして出力する
;;; 呼び出し方:
;;;   casl2 -M 1024 five_ccw90_pbm.casl $CASL2LIB/pbm16.casl $CASL2LIB/outb_w.casl $CASL2LIB/divl.casl $CASL2LIB/rev.casl rotate_ccw90.casl >five_ccw90.pbm
MAIN    START
        RPUSH
        LAD     GR1,GRAPH
        LAD     GR2,RES
        CALL    ROTATE
        LAD     GR1,RES
        CALL    PBM16
        RPOP
FIN     RET
GRAPH   DC      #FFFC
        DC      #FFFC
        DC      #FFFC
        DC      #0700
        DC      #0700
        DC      #0700
        DC      #FFFC
        DC      #FFFC
        DC      #FFFC
        DC      #071C
        DC      #071C
        DC      #071C
        DC      #FFFF
        DC      #FFFF
        DC      #FFFF
        DC      #0000
RES     DS      16
        END
