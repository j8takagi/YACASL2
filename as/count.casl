COUNT1	START
;       入力    GR1:検索する語
;       処理    GR1中の'1'のビットの個数を求める
;       出力    GR0:GR1中の'1'のビットの個数
	PUSH	0,GR1
    PUSH	0,GR2
    SUBA	GR2,GR2
    AND		GR1,GR1
    JZE 	RETURN
MORE LAD	GR2,1,GR2
	LAD		GR0,-1,GR1
    AND		GR1,GR0
    JNZ		MORE
RETURN LD	GR0,GR2
	POP		GR2
    POP		GR1
    RET
    END
