MAIN    START
        LAD     GR1,A
        LD      GR0,B
        LD      GR2,P
        LD      GR3,Q
        CALL    REPLACE
        LAD     GR1,A
        LAD     GR2,0
LOOP    CPA     GR2,CNT
        JZE     FIN
        LD      GR1,A,GR2
        CALL    OUTL
        LAD     GR2,1,GR2
        JUMP    LOOP
FIN     RET
A       DC      #FFFF,#FFFF,#FFFF,#FFFF,#FFFF,#FFFF
CNT     DC      6        
B       DC      #B1D0
P       DC      55
Q       DC      12
        END
