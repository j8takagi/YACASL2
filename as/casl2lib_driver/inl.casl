;;; casl2 inl.casl ../casl2lib/inl.casl ../casl2lib/outl.caslg
MAIN    START
LOOP    CALL    INL             ; GR1に数値、GR0に文字列の長さ
        JOV     NAN
        AND     GR0,GR0
        JZE     FIN
        CALL    OUTL
        JUMP    LOOP
NAN     OUT     NANMSG,MLEN
        JUMP    LOOP
FIN     RET
NANMSG  DC      'Not A Number'
MLEN    DC      12
        END
