PTN4    START
        ST      GR1,WRK
        LD      GR1,GR2
        CALL    PTN1
        PUSH    0,GR2       ; PTN1 の結果を一時保存
        LD      GR1,WRK
        LAD     GR1,1,GR1   ; ビット番号の調整
        CALL    PTN2
        POP     GR1         ; 保存していた PTN1 の結果を GR1 ヘ
        XOR     GR2,GR1
        LD      GR1,WRK
        RET
WRK     DS      1
        END
