;;; 「五」を90度回転させた16 x 16のpbm画像を出力する
;;; 呼び出し方:
;;;   casl2 call_rotate2.casl rotate2.casl pbm16.casl
MAIN    START
        LAD     GR1,GRAPH
        LAD     GR2,RES
        LAD     GR3,8
        CALL    ROTATE
        LAD     GR1,RES
        CALL    PBM16
        RET
GRAPH   DC      #FFFC
        DC      #FFFC
        DC      #FFFC
        DC      #0700
        DC      #0700
        DC      #0700
        DC      #FFFC
        DC      #FFFC
        DC      #FFFC
        DC      #071C
        DC      #071C
        DC      #071C
        DC      #FFFF
        DC      #FFFF
        DC      #FFFF
        DC      #0000
RES     DS      16
        END
