;;; OUTの使用例
OTEST   START
        OUT     BUF1,L1
        OUT     BUF2,L2
        OUT     BUF3,L3
        OUT     BUF4,L4
        RET
BUF1    DC      '0',9,'1',10            ; TAB、改行
L1      DC      4
BUF2    DC      ' !"#$%&''()*+,-./0123456789:;<=>?'
L2      DC      32
BUF3    DC      '@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\]^_'
L3      DC      32
BUF4    DC      '`abcdefghijklmnopqrstuvwxyz{|}~'
L4      DC      31
        END
