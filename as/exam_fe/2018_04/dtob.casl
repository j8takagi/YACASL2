DTOB    START
        RPUSH
        ADDL   GR2,GR1
        LAD    GR0,0           ; 戻り値の初期化
LP      CPL    GR1,GR2         ; 変換終了？
        JZE    FIN
        LD     GR4,0,GR1       ; 数字1文字の取出し
        SUBL   GR4,='0'        ; 1 桁を数値に変換
        SLL    GR0,1           ; GR0を10倍してGR4を加算
        LD     GR5,GR0
        SLL    GR5,2
        ADDL   GR0,GR5
        ADDL   GR0,GR4
        LAD    GR1,1,GR1
        JUMP   LP
FIN     RPOP
        RET
        END
