; need outi.casl
MAIN    START
        LAD GR2,0
        LD  GR3,CNT
LOOP    LD GR1,VAL,GR2
        CALL    OUTI
        LAD GR2,1,GR2
        CPA GR2,GR3
        JZE FIN
        JUMP LOOP
FIN RET
        END
VAL DC  #0000,#0001,#0002,#0010,#0100,#1000,#7FFF,#FFFF,#FFFE,#FFF0,#FF00,#F000,#8001,#8000
CNT DC  14
