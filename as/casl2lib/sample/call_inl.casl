;;; casl2 inl.casl ../casl2lib/inl.casl ../casl2lib/outl.casl
MAIN    START
LOOP    CALL    INL             ; GR1に数値、GR2に文字列の長さ
        JOV     NAN
        JMI     BIG
        LD      GR2,GR2
        JZE     FIN
        CALL    OUTL
        JUMP    LOOP
NAN     OUT     NANMSG,NANLEN
        JUMP    LOOP
BIG     OUT     BIGMSG,BIGLEN
        JUMP    LOOP
FIN     RET
NANMSG  DC      'Not a number'
NANLEN  DC      12
BIGMSG  DC      'Over 65535'
BIGLEN  DC      10
        END
